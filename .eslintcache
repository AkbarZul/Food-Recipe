nt() {\r\n    this.getPopularRecipe();\r\n  }\r\n\r\n  render() {\r\n    const { PopularRecipe, RecipeImg } = this.state;\r\n    return (\r\n      <main>\r\n        <div style={{ float: \"none\" }}></div>\r\n        <div className='container container-sm'>\r\n          <div className={`${css.HeadLine} mb-5`}>\r\n            <h2>Popular For You!</h2>\r\n          </div>\r\n          <div className={css.HeroImage}>\r\n            <img src={RecipeImg[0]} alt='ramen' className='img-fluid rounded' />\r\n          </div>\r\n          <div className={css.HeroText}>\r\n            <h2>{PopularRecipe.title_rcp}</h2>\r\n            <p className={`${css.LongParagraph} mb-4`}>\r\n              {PopularRecipe.desc_rcp}\r\n            </p>\r\n            <button type='button' className={`btn btn-warning ${css.Btn}`}>\r\n              Learn More\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Popular;","E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\NewRecipe\\NewRecipe.jsx",["174"],"import React from \"react\";\r\nimport axios from \"axios\";\r\n\r\nimport css from \"./NewRecipe.module.css\";\r\nimport Burger from \"../../assets/images/burger.png\";\r\n\r\nclass NewRecipe extends React.Component {\r\n  state = {\r\n    NewRecipe: {},\r\n    RecipeImg: {}\r\n  }\r\n\r\n  getNewRecipe = () => {\r\n    axios\r\n      .get(`${process.env.REACT_APP_URL}/recipes`)\r\n      .then(({data}) => {\r\n        console.log(JSON.parse(data.data[2].img_rcp));\r\n        this.setState({\r\n          NewRecipe: data.data[0],\r\n          RecipeImg: JSON.parse(data.data[0].img_rcp),\r\n        });\r\n      })\r\n      .catch(err => console.error(err));\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.getNewRecipe();\r\n  }\r\n\r\n  render() {\r\n    const { NewRecipe, RecipeImg } = this.state;\r\n    return (\r\n      <main id={css.Main}>\r\n        <div style={{ float: \"none\" }}></div>\r\n        <di