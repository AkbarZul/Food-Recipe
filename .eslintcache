orage.setItem(\"userId\", res.data.data);\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n    this.props.history.push(\"/code\");\r\n  };\r\n  render() {\r\n    console.log(this.state.email_user);\r\n    return (\r\n      <div className=\"container-fluid h-100\">\r\n        <div className=\"row\">\r\n          <div\r\n            className=\"position-relative d-none d-md-block col-md-4 col-lg-6 p-0\"\r\n            style={{ backgroundColor: \"yellow\" }}\r\n          >\r\n            <img\r\n              src={Background}\r\n              alt=\"background\"\r\n              style={{ objectFit: \"cover\", objectPosition: \"center\" }}\r\n              className=\"w-100 full\"\r\n            />\r\n            <div className=\"mask h-100 w-100 d-flex justify-content-center align-items-center\">\r\n              <img src={Mama} alt=\"logo\" />\r\n            </div>\r\n          </div>\r\n          <div className=\"full d-flex justify-content-center align-items-center col-12 col-md-8 col-lg-6\">\r\n            <div className=\"col-10 col-md-8 col-xl-6 d-flex flex-column justify-content-center align-items-center font-weight-medium\">\r\n              <h2 className=\"main font-weight-bold mt-n5\">Forgot Password?</h2>\r\n              <span\r\n                className=\"log mt-4 mb-4 font-weight-normal text-center\"\r\n                style={{ fontSize: \"15px\" }}\r\n              >\r\n                We just need your registered e-mail address to send your\r\n                password resend\r\n              </span>\r\n              <Form className=\"w-100 mb-3 mt-3\" onSubmit={this.sendMail}>\r\n                <Form.Group controlId=\"formBasicEmail\">\r\n                  <Form.Label>Email</Form.Label>\r\n                  <Form.Control\r\n                    type=\"email\"\r\n                    placeholder=\"Email\"\r\n                    name=\"email_user\"\r\n                    className=\"pt-4 pb-4 pl-4 pr-0 input\"\r\n                    onChange={this.handlerChange}\r\n                    required\r\n                  />\r\n                </Form.Group>\r\n                <Button\r\n                  variant=\"warning\"\r\n                  type=\"submit\"\r\n                  className=\"w-100 btn-main pt-2 pb-2 font-weight-medium\"\r\n                >\r\n                  Send Email\r\n                </Button>\r\n              </Form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n",["159","160"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\Homepage\\Homepage.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\auth\\Register.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\Profile\\Profile.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\recipes\\DetailVideo.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\chat\\Chat.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\recipes\\DetailRecipe.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\auth\\Index.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\recipes\\addRecipe.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\NotFound\\NotFound.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\pages\\search\\Search.jsx",["161"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\PrivateRoute.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\redux\\actionCreators\\auth.js",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Header\\Header.jsx",["162"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Popular\\Popular.jsx",["163"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Navbar\\Navbar.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\NewRecipe\\NewRecipe.jsx",["164"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Footer\\Footer.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Profile\\Profile.jsx",[],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Chat\\Chat.jsx",["165","166"],"E:\\live-coding\\New folder\\get fe\\Food-Recipe\\src\\components\\Detail\\Detail.jsx",["167"],"import React, { Component } from \"react\";\r\nimport { Container } from \"react-bootstrap\";\r\nimport { withRouter, Link } from \"react-router-dom\";\r\nimport { connect } from \"react-redux\";\r\nimport axios from \"axios\";\r\n\r\nimport detail from \"./Detail.module.css\";\r\nimport { getSingleRecipe } from \"../../redux/actionCreators/Recipes\";\r\n// import Image from '../../assets/4da51338c06dd21688b82eae3bc9dfa6.jpg'\r\n// import Loading from '../../assets/gifs/spinner.gif'\r\nimport LikedIcon from \"../../assets/icons/like.png\";\r\nimport SavedIcon from \"../../assets/icons/saved.png\";\r\nimport PlayIcon from \"../../assets/icons/play.png\";\r\nimport PhotoUser from \"../../assets/photo-comment.png\";\r\n\r\nclass Detail extends Component {\r\n  state = {\r\n    recipe: {},\r\n    imgRecipe: \"\",\r\n    videoRecipe: [],\r\n    idRecipe: 0,\r\n    comments: [],\r\n    addComment: \"\",\r\n    msg: \"\",\r\n  };\r\n\r\n  getRecipeById = async () => {\r\n    const { id } = this.props.match.params;\r\n    this.setState({\r\n      idRecipe: id,\r\n    });\r\n    await this.props.dispatch(getSingleRecipe(id));\r\n\r\n    const { recipes } = this.props;\r\n    if (recipes.singleRecipe.msg) {\r\n      this.props.history.push(\"/recipe\");\r\n    } else {\r\n      this.setState({\r\n        recipe: recipes.singleRecipe.data[0],\r\n      });\r\n      const image = JSON.parse(this.state.recipe.img_rcp)[0];\r\n      this.setState({\r\n        imgRecipe: image,\r\n      });\r\n      const video = JSON.parse(this.state.recipe.video_rcp);\r\n      this.setState({\r\n        videoRecipe: video,\r\n      });\r\n    }\r\n    // console.log(this.state)\r\n  };\r\n\r\n  getCommentByRecipe = async () => {\r\n    const { id } = this.props.match.params;\r\n    await axios\r\n      .get(`http://localhost:5000/comments/${id}`)\r\n      .then((res) => {\r\n        console.log(res);\r\n        this.setState({\r\n          comments: res.data.comment,\r\n        });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  handlerChange = (e) => {\r\n    const value = e.target.value;\r\n    this.setState({\r\n      addComment: value,\r\n    });\r\n  };\r\n\r\n  addComment = async () => {\r\n    const { id } = this.props.match.params;\r\n    const userid = await localStorage.getItem(\"userId\");\r\n    const data = {\r\n      user_id: userid,\r\n      recipe_id: id,\r\n      comment: this.state.addComment,\r\n    };\r\n    await axios\r\n      .post(\"http://localhost:5000/comments\", data)\r\n      .then((res) => {\r\n        console.log(res);\r\n        this.setState({\r\n          like: true,\r\n        });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n    this.setState({\r\n      addComment: \"\",\r\n    });\r\n    this.getCommentByRecipe();\r\n  };\r\n\r\n  addLike = async () => {\r\n    const { id } = this.props.match.params;\r\n    const userid = await localStorage.getItem(\"userId\");\r\n    const data = {\r\n      recipe_id: id,\r\n      user_id: userid,\r\n    };\r\n    axios\r\n      .post(\"http://localhost:5000/likes\", data)\r\n      .then((res) => {\r\n        console.log(res);\r\n        this.setState({\r\n          msg: res.data.msg,\r\n        });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  addSave = async () => {\r\n    const { id } = this.props.match.params;\r\n    const userid = await localStorage.getItem(\"userId\");\r\n    const data = {\r\n      recipe_id: id,\r\n      user_id: userid,\r\n    };\r\n    axios\r\n      .post(\"http://localhost:5000/saves\", data)\r\n      .then((res) => {\r\n        console.log(res);\r\n        this.setState({\r\n          msg: res.data.msg,\r\n        });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  componentDidMount = () => {\r\n    this.getRecipeById();\r\n    this.getCommentByRecipe();\r\n  };\r\n\r\n  render() {\r\n    const { isPending } = this.props.recipes;\r\n    const { comments } = this.state;\r\n    console.log(this.state.msg);\r\n\r\n    return (\r\n      <Container>\r\n        {/* animasi loading */}\r\n        {/* { isPending && <div className={ detail.Loading }>\r\n\t\t\t\t\t<img src={Loading} alt=\"\"/>\r\n\t\t\t\t</div> } */}\r\n        {/* comment aja klo ga mau dipake, blom fix juga soalnya */}\r\n        <div className=\"text-center\">\r\n          <h1 className={\"mx-auto \" + detail.Title}>\r\n            {isPending ? \"Loading...\" : this.state.recipe.title_rcp}\r\n          </h1>\r\n        </div>\r\n        <div\r\n          className={\"mx-auto \" + detail.ImageSize}\r\n          style={{\r\n            backgroundImage: `url(${!isPending && this.state.imgRecipe})`,\r\n          }}\r\n        >\r\n          <div className={detail.ButtonList}>\r\n            <div className={detail.SavedButton}>\r\n              <img src={SavedIcon} alt=\"\" onClick={this.addSave} />\r\n            </div>\r\n            <div className={detail.LikedButton}>\r\n              <img src={LikedIcon} alt=\"\" onClick={this.addLike} />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div className={\"mx-auto \" + detail.Description}>\r\n          <h2 className={detail.TextDesc}>Ingredients</h2>\r\n          <div className={detail.StepList}>\r\n            <span style={{ whiteSpace: \"pre-line\" }}>\r\n              {!isPending && this.state.recipe.ingridients_rcp}\r\n            </span>\r\n            <br />\r\n            <span>{!isPending && this.state.recipe.desc_rcp}</span>\r\n  